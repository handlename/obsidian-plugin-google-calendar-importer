{
	"name": "google-calendar-importer",
	"version": "0.1.0",
	"description": "Import Google Calendar events into Daily Notes. Supports customizable templates and timezone conversion.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"release": "npm run build && node scripts/prepare-release.mjs"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@types/mustache": "^4.2.6",
		"@types/node": "^24.0.0",
		"@vitest/ui": "^4.0.6",
		"builtin-modules": "^4.0.0",
		"esbuild": "^0.24.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "5.9.3",
		"vitest": "^4.0.6"
	},
	"dependencies": {
		"date-fns": "^4.1.0",
		"date-fns-tz": "^3.2.0",
		"googleapis": "^140.0.1",
		"mustache": "^4.2.0"
	}
}
